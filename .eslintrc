{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "semi": 2,
    "comma-dangle": [2, "always-multiline"],
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0
  }
}
